{% for node in site.pages %}
{% if node.url == '/' %}{{ next }}{% else %}
{% assign _node_path = node.url %} path = {{ _node_path }}<br>
{% assign _node_depth = node.url | split: '/' | size | minus:1 %} depth = {{ _node_depth }}<br>
{% assign _node_child_depth = node.url | split: '/' | size %} child depth = {{ _node_child_depth }}<br>
{% assign _node_parent_depth = node.url | split: '/' | size | plus:1 | %} parent depth = {{ _node_parent_depth }}<br>
{% for section in node.url %}{% assign _node = node.url | split: '/' | last %} node = {{ _node }}<br>{% assign _node_path_reversed = section | split: '/' | reverse %}
{% for _directory in _node_path_reversed %}{% if forloop.index == 2 %}{% assign _node_parent = _directory %}parent = {{ _node_parent }}{% endif %}{% endfor %}
{% endfor %}{% endif %}{% endfor %}
