{% assign sorted = site.pages | sort:"url" %}
{% for node in sorted offset:1 %}
  path: {{node.url}}
  {% assign slugs = node.url | split: '/' %}
  {% for slug in slugs %}{{slug}}{% endfor %}
    {% assign index = forloop.index0 %}
    {% for level in index offset:1 %}{{level}}{% endfor %}
{% endfor %}
