{% for node in sorted offset:1 %}{% assign slugs = node.url | split: '/' %}
{% for slug in slugs[node.first] %}
{% if node.url != page.url %}
<br>nodefirst={{ node.url }}<br>
{% endif %}{% endfor %}{% endfor %}
