{% assign sorted = site.pages | sort:"url" %}{% 
for node in sorted offset:1 %}
  {% capture PATH %}{{node.url | remove:.html}}{% endcapture %}<b>{{PATH}}<b>
  {% assign slugs = node.url | split: '/' %}
  {% for slug in slugs %}
    {% capture SLUG %}{{ slug | remove: .html}}{% endcapture %}{{SLUG}}<br>
  {% endfor %}
  <br>
{% endfor %}
